<paquet
	prefix="tutotheque"
	categorie="squelette"
	version="1.0.0"
	etat="dev"
	compatibilite="[3.2.3;3.2.*]"
	logo="prive/themes/spip/images/tutotheque-64.png"
	documentation="https://github.com/abelass/tutotheque"
>
	<!--
		Paquet généré le 2019-03-08 15:34:54
		(Vous pouvez bien entendu supprimer ces commentaires) 
	-->

	<nom>Tutothèque</nom>
	<!-- Votre tutothèque -->

	<!-- cette balise peut être présente plusieurs fois s'il y a plusieurs auteurs --> 
	<auteur lien='https://web-simple.eu'>Rainer Müller</auteur>



	<licence>GNU/GPL v3</licence>

	<!--
		Exemple de commandes
		* dependances
		<necessite nom="saisies" compatibilite="[2.28.0;]" />
		<utilise nom="saisies" compatibilite="[2.28.0;]" />
		<lib nom="simplehtmldom" lien="http://chemin_vers_archive.zip" />
		* pipelines
		<pipeline nom="autoriser" inclure="tutotheque_autorisations.php" />
		<pipeline nom="insert_head" inclure="tutotheque_pipelines.php" />
		* declarer un pipeline 
		<pipeline nom="mon_pipeline" action="" />
		* parents de menus de SPIP :
				menu_accueil (à éviter), menu_edition, menu_publication,
				menu_activite, menu_squelette, menu_administration, menu_configuration 
		<menu nom="tutotheque" titre="tutotheque:tutotheque_titre" parent="menu_edition"  icone="images/tutotheque-16.png" />
		* parents d'onglets... configuration, plugins, statistiques, ... 
		<onglet nom="tutotheque" titre="tutotheque:tutotheque_titre" parent="configuration" icone="images/tutotheque-24.png" />
	-->

	<necessite nom="wsps_html5" compatibilite="[2.0.10;]" />
	<necessite nom="wsp_ml" compatibilite="[0.2.14;]" />
	<necessite nom="tutoriel" compatibilite="[1.0.0;]" />


</paquet>
